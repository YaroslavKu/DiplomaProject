  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for watchOSApp WatchKit App
  pod 'Firebase/Database'
end

target 'watchOSApp WatchKit Extension' do
  # Comment the next line if you don't want to use dynamic frameworks
